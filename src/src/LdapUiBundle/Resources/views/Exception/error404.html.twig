{% extends 'LdapUiBundle::layout.html.twig' %}

{% block title %}{{ 'Page not found' | trans('error')  }}{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-6 mx-auto">
                    <div class="loginError card rounded-0 text-white bg-warning">
                        <div class="card-header">
                            <h3 class="mb-0">{{ 'Page not found' | trans('error') }}</h3>
                        </div>
                        <div class="card-body">
                            The requested page couldn't be located. Checkout for any URL
                            misspelling or <a href="{{ path('index') }}">return to the browser</a>.
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}